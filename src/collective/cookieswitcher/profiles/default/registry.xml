<?xml version="1.0"?>
<registry
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.cookieswitcher">
	<record name="collective.cookieswitcher.matomo_url">
	  <field type="plone.registry.field.TextLine">
	    <title>Matomo url</title>
	    <description></description>
	    <required>True</required>
	  </field>
	  <value>https://matomourl.com/</value>
	</record>
  <record name="collective.cookieswitcher.data_handling_url">
    <field type="plone.registry.field.TextLine">
      <title>Data handling URL</title>
      <description></description>
      <required>True</required>
    </field>
    <value>/de/datenschutz</value>
  </record>

</registry>
